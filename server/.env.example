# Server configuration
PORT=5000
NODE_ENV=development

# MongoDB configuration (legacy)
# MONGODB_URI=mongodb://localhost:27017/maplescan
# MONGODB_URI=mongodb+srv://username:password@cluster0.mongodb.net/maplescan?retryWrites=true&w=majority

# Supabase configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key

# Logging
LOG_LEVEL=info

# API Keys for barcode databases
# UPC Database API (SearchUPC)
UPC_DATABASE_API=https://api.searchupc.com/v1
UPC_DATABASE_API_KEY=your_api_key

# Go-UPC API (use sparingly due to potential costs)
GO_UPC_API=https://api.go-upc.com/v1
GO_UPC_API_KEY=your_api_key

# Google Custom Search API for web search
GOOGLE_API_KEY=your_google_api_key
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id 